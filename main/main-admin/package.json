    "start": "nodemon -w src -x babel-node index",
    "dev:server": "NODE_ENV=dev nodemon -w lib -x babel-node tools/dev",
    "production:server": "NODE_ENV=production nodemon -w lib -x babel-node tools/dev",
    "dev:webpack": "webpack-dev-server -d",
    "dev:copy": "mkdir -p dist/static/main/resource && cp -r static/resource dist/static/main/",
    "dev:browser-sync": "browser-sync start --proxy localhost:4001 --port 3000 -f static/main/style",
    "dev": "NODE_ENV=dev concurrently 'npm run dev:server' 'npm run dev:webpack' 'npm run dev:copy' 'npm run dev:browser-sync'",
    "production": "NODE_ENV=production concurrently 'npm run production:server' 'webpack -d' 'npm run dev:copy'",
    "build0": "concurrently 'webpack' 'copyfiles -u 1 static/*.html dist/static/main/' 'npm run dev:copy'"
  },
